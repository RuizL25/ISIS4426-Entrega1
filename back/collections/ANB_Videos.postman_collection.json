{
    "info": {
      "name": "ANB Video API",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Subir Video",
        "request": {
          "method": "POST",
          "header": [],
          "body": {
            "mode": "formdata",
            "formdata": [
              {
                "key": "title",
                "value": "Mi primer video",
                "type": "text"
              },
              {
                "key": "file",
                "type": "file",
                "src": "/ruta/a/tu/video.mp4"
              }
            ]
          },
          "url": {
            "raw": "http://localhost:8080/api/videos",
            "protocol": "http",
            "host": ["localhost"],
            "port": "8080",
            "path": ["api", "videos"]
          }
        },
        "response": []
      },
      {
        "name": "Consultar Job Status",
        "request": {
          "method": "GET",
          "header": [],
          "url": {
            "raw": "http://localhost:8080/api/jobs/{{job_id}}",
            "protocol": "http",
            "host": ["localhost"],
            "port": "8080",
            "path": ["api", "jobs", "{{job_id}}"]
          }
        },
        "response": []
      },
      {
        "name": "Listar Videos Procesados",
        "request": {
          "method": "GET",
          "header": [],
          "url": {
            "raw": "http://localhost:8080/api/videos",
            "protocol": "http",
            "host": ["localhost"],
            "port": "8080",
            "path": ["api", "videos"]
          }
        },
        "response": []
      },
      {
        "name": "Obtener Video por ID",
        "request": {
          "method": "GET",
          "header": [],
          "url": {
            "raw": "http://localhost:8080/api/videos/{{video_id}}",
            "protocol": "http",
            "host": ["localhost"],
            "port": "8080",
            "path": ["api", "videos", "{{video_id}}"]
          }
        },
        "response": []
      }
    ],
    "variable": [
      {
        "key": "job_id",
        "value": "cambia-por-id-devuelto"
      },
      {
        "key": "video_id",
        "value": "1"
      }
    ]
  }
  